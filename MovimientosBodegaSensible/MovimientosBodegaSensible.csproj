    <ApplicationIcon>wwwroot\favicon.ico</ApplicationIcon>
    <Copyright>Lextrack</Copyright>
    <FileVersion>1.0.9</FileVersion>
    <AssemblyVersion>1.0.9</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EPPlus" Version="7.0.10" />
